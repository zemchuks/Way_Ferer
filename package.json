{
  "name": "way_ferer",
  "version": "1.0.0",
  "description": "A public bus transportation booking server",
  "main": "server.js",
  "scripts": {
    "start": "babel-node ./build/server.js",
    "createTable": "node ./build/db/runcreatetables.js",
    "createTable-dev": "babel-node ./server/db/runcreatetables.js",
    "populatetables": "node ./build/db/runpopulate.js",
    "populatetables-dev": "babel-node ./server/db/runpopulate.js",
    "droptables": "babel-node ./server/db/dropTables.js",
    "test": "nyc mocha --reporter spec --require babel-register --require babel-polyfill ./server/test/ --timeout=100000 --exit",
    "build": "npm run createTable && babel server -d build",
    "start:dev": "nodemon server.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zemchuks/Way_Ferer.git"
  },
  "author": "Bruno Ezemba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zemchuks/Way_Ferer/issues"
  },
  "homepage": "https://github.com/zemchuks/Way_Ferer#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "braces": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier-eslint-cli": "^5.0.0"
  }
}
